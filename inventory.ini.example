[miners]
# 서버 정보 설정
# 실제 사용시 이 파일을 복사하여 inventory.ini로 이름을 변경하고 실제 서버 정보를 입력하세요
# 형식: 서버명 ansible_host=IP주소 node_id=노드ID

# 예시:
# contabo1 ansible_host=192.168.1.100 node_id=7006171
# contabo2 ansible_host=192.168.1.101 node_id=7096264

# 환경 변수를 사용한 설정 (고급 사용자용)
# contabo1 ansible_host="{{ lookup('env', 'CONTABO1_HOST') }}" node_id="{{ lookup('env', 'CONTABO1_NODE_ID') }}" 